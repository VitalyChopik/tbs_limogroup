{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body class="{{body_class}}" data-template="base.twig">
		{% block header %}
			{{ include('partials/site_header.twig') }}
		{% endblock %}

		<main id="content" role="main" class="content-wrapper">
			{% block content %}
			{% endblock %}
		</main>

		{% block footer %}
			{{ include('partials/site_footer.twig') }}
			{{ function('wp_footer') }}
		{% endblock %}
	</body>
</html>